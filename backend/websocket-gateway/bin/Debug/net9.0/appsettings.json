{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "Cors": {
    "AllowedOrigin": "http://localhost:3000"
  },
  "AllowedHosts": "*",
  "MassageBrokerConnections": {
    "HostName": "localhost",
    "Port": 5672
  },
  "MQProducerOptions": {
    "ExchangeName": "subs.exchange",
    "ExchangeType": "direct",
    "Bindings": [
      {
        "QueueName": "subs.queue",
        "RoutingKey": "sub"
      },
      {
        "QueueName": "unsubs.queue",
        "RoutingKey": "unsub"
      }
    ]
  },
  "MQConsumerOptions":{
    "ExchangeName": "stock_data"
  }
}